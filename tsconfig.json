{
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "lib": [
      "es2021"
    ],
    "target": "es2021",
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*",
  ]
}
